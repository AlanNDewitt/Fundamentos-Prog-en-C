
#include <stdio.h>
#include <math.h>

lectura( int *V ) 
{
    
    for( int i=0; i<10 ;i++ ) {
        
        printf("ingresa el %i valor: ",i+1 );
        scanf("%i", &V[i] );
        
        
    }
    
    
    for( int i=0; i<10 ;i++ ) {
        
        printf("\n el %i valor: %i ",i+1,V[i] );
        
        
        
    }
    
    
}

suma(int *V , int *t) {
    
     
     
     for(int i=0; i<10;i++) {
        
       *t += pow(V[i],2);
        
        
    }
  
    
}



int main()

{
   int Vec[10] , T=0;
   
   lectura(Vec);
   
    suma(Vec,&T);
    
    printf("\n el resultado es %i",T);
    
    
    
}

